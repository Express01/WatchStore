@model List<WristWatchViewModel>
<h1>Результаты поиска</h1>
@if(Model.Count==0)
{
    <p>Ничего не найдено</p>
}
else
{
   @foreach(var watch in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelwatch=>watch.Name)
            </td>
            <td>
                @Html.DisplayFor(modelwatch=>watch.BrendName)
            </td>
            <td>
                @Html.DisplayFor(modelwatch=>watch.Price)
            </td>
            <td>
               <img src="@Html.DisplayFor(modelwatch=>watch.Picture)">
            </td>
            <td>
                @Html.DisplayFor(modelwatch=>watch.Gender)
            </td>
        </tr>
    }
}